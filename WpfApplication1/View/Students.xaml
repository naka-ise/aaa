        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Students Area" Height="300" Width="300" Background="#FFDEB180">
    <Canvas>
        <Button Content="Update Student's Info" HorizontalAlignment="Left" Margin="88,100,0,0" VerticalAlignment="Top" Width="130" FontFamily="Cooper Black" FontSize="10" Foreground="#FF0E3574" Background="#FFC7D2E4"/>
        <Button Content="Delete Student's Profile" HorizontalAlignment="Left" Margin="88,160,0,0" VerticalAlignment="Top" Width="130" FontFamily="Cooper Black" FontSize="10" Foreground="#FF0E3574" Background="#FFC7D2E4"/>
        <Button Content="Get Student's Feedback" HorizontalAlignment="Left" Margin="88,220,0,0" VerticalAlignment="Top" Width="130" FontFamily="Cooper Black" FontSize="10" Foreground="#FF0E3574" Background="#FFC7D2E4"/>
        <Label Content="Student's Managment" HorizontalAlignment="Center" Margin="12,10,8,0" VerticalAlignment="Top" Height="38" Width="275" FontFamily="Cooper Black" FontSize="24" Foreground="#FF0E3574"/>
    </Canvas>
</Window>
